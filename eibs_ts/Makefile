#======================================================================================================#
# Makefile for prototype Ethereum Input Bus supplier implemented in TypeScript
#======================================================================================================#

.PHONY: all pre_tsc post_tsc run test clean

#======================================================================================================#

all: pre_tsc post_tsc

pre_tsc:

post_tsc:

run:
	node src/index.js

test:
	mocha

clean:
	rm -f disk_cache/*

